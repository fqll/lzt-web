@import '/common/components/cateTab/cateTab.wxss';
.list-wrap{
  width: 90%;
  margin-left: 5%;
  border-top: 1px solid rgba(228, 228, 228, 1)
}
.each-list{
  width: 100%;
  height: 116rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 24rpx;
  border-bottom: 1px solid rgba(228, 228, 228, 1)
