.list-wrap{
  width: 90%;
  margin-left: 5%;
  margin-top: 80rpx;
}
.each-list{
  padding: 20rpx 0;
  border-bottom: 1px solid rgba(228, 228, 228, 1);
  display: flex;
  flex-direction: row;
  justify-content: space-between
}
.each-list text{
  font-size: 28rpx;
  color: black;
}
.each-list text text{
  color: rgba(55, 163, 75, 1);
}
.each-list view{
  padding: 2rpx 10rpx;
  border-radius: 10rpx;
  font-size: 24rpx;
  border: 1px solid rgba(55, 163, 75, 1);
  color: rgba(55, 163, 75, 1)
}
.each-list .main{
  background-color: rgba(55, 163, 75, 1);
  color: white
}
.link-wrap{
  margin-top: 80rpx;
  margin-left: 5%;
  font-size: 28rpx;
}
.link-wrap text{
  width: auto;
  display: table;
  color: #996699;
  border-bottom: 1px solid #996699;
  margin-bottom: 30rpx;
}
.tool-btn{
  position: fixed;
  width: 124rpx;
  right: 50rpx;
  bottom: 50rpx;
  z-index: 1
}
.tool-btn view{
  width: 100%;
  height: 60rpx;
  background-color: rgba(102, 102, 102, 1);
  border-radius: 56rpx;
  font-size: 26rpx;
  color: white;
  text-align: center;
  line-height: 60rpx;
}
.tool-btn view:first-child{
  margin-bottom: 30rpx;
}
