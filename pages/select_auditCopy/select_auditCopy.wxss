@import '/common/css/normal.wxss';
.search-wrap{
  width: 90%;
  height: 60rpx;
  margin-left: 5%;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-radius: 10rpx;
  font-size: 24rpx;
  color:#999999;
  background-color: rgba(246, 246, 246, 1);
  box-sizing: border-box;
  padding: 0 3%;
  margin-bottom: 20rpx;
}
.search-wrap image{
  width: 30rpx;
}
.search-wrap input{
  width:60%;
  margin-left: 30rpx;
}

.table-view{
  width: 90%;
  margin-left: 5%;
  color:rgb(102, 102, 102);
  font-size: 24rpx;
  border: 1px solid rgb(215,215,215);
  border-bottom: none;
  margin-top: 20rpx;
}
.table-view .head{
  font-size: 24rpx;
  color:black
}
.table-view .head,.table-view .content{
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid rgb(215,215,215)
}
.table-view .head view,.table-view .content view{
  width: 50%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-right: 1px solid rgb(215,215,215)
}
.table-view .head view:last-child,.table-view .content view:last-child{
  width: 50%;
  border-right: none
}
.table-view .content view:last-child{
  color: rgb(0, 153, 204)
}
.highlight{
  position: relative;
  z-index: 10001;
  background-color: white
}
