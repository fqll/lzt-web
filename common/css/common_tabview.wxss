.common-table-view{
  width: 100%;
  color:rgb(51,51,51);
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center
}

.common-table-view .head,.common-table-view .content{
  display: flex;
  flex-direction: row;
  font-size: 28rpx;
  border-radius: 4rpx;
  border: 1px solid #999999
}

.common-table-view .content{
  border: 1px solid #999999;
  border-bottom: none
}

.common-table-view .head view{
  height: 70rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-right: 1px solid #999999;
  color: #333
}

.common-table-view .content:nth-child(2){
  border-top: none
}

.common-table-view .content:last-child{
  border-bottom: 1px solid #999999
}

.common-table-view .content view{
  text-align: center;
  line-height: 70rpx;
  height: 70rpx;
  color: #666666;
  border-right: 1px solid #999999;
}
.common-table-view .content .grayer{
  color: #999999;
}

.common-table-view .content .common-table-btn {
  color: #3196ff
}

.common-table-view .content view:last-child{
  border-right: none;
}

.pre-logo{
  width: 30rpx;
}

.common-table-view .head view:last-child{
  border-right: none
}

.common-table-view .content .single-btn{
  width: 100rpx;
  height: 40rpx;
  border-radius: 4rpx;
  background-color: #4885fa
}